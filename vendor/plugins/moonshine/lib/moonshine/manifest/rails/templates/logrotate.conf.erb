<%= log_or_glob %> {
  <% options[:options].each do |o| %>
  <%= o %>
  <% end %>
  <% unless options[:postrotate].nil? %>
  postrotate
    <%= options[:postrotate] %>
  endscript
  <% end %>
  <% unless options[:prerotate].nil? %>
  prerotate
    <%= options[:prerotate] %>
  endscript
  <% end %>
}